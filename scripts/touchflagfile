#!/bin/bash

#+++___________________________________________________________________________________
#
#
# Script name:                  touchflagfile
#
# Description:			Touch a flagfile
#
# Parameters:
#
# Environment:
#
# Return codes:
#
# Modification history:
#
#       15-Apr-2004     1.0     oracle    Original version
#
#+++___________________________________________________________________________________

CS_TOP=${CS_TOP:-/opt/dsp}
. ${CS_TOP}/env/dsp.env
. ${CS_TOP}/env/funcs.sh

DEL_TEMPFILES="NO"

#
# Script starts here
#
#
#set_error_trap

FLAGFILE=${tmpfile}.flagfile

touch ${FLAGFILE}

if [ -f ${FLAGFILE} ]
then
    echo ${tmpfile}.flagfile
fi


exit $EXITCODE

